{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "sourceMap": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "noEmit": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}
